<h1>Thank you for downloading PhpStorm!</h1>

<div class="box nota_bene licenseKey">
    <p>Your download should start shortly. If it doesn't, please <a href="http://download.jetbrains.com/webide/PhpStorm-5.0.4.tar.gz">click here</a>.</p>
</div>
<br/>
<p class="leftfloated block55">Enter your e-mail to be notified when a new version becomes available:</p>
<div class="leftfloated">
    


<script type='text/javascript' src='/eforms/dwr/interface/SubscriptionService.js'></script>
<script type='text/javascript' src='/eforms/dwr/engine.js'></script>

<script type="text/javascript">
    function trim(str) {
        return str.replace(/^\s+|\s+$/g, '');
    }
    function errMsg(state) {
        var msg = document.getElementById('error-msg');
        if (state)msg.style.display = 'block';
        else msg.style.display = 'none';
    }
    function validateEmail(valfield) {
        var tfld = trim(valfield.value);
        var email = /^[^@]+@[^@.]+\.[^@]*\w\w$/  ;
        if (!email.test(tfld)) {
            errMsg(true);
            return false;
        }
        errMsg(false);
        return true;
    }
    function Proceed() {
        var el = document.getElementById('emaiForSubscription');
        if (!validateEmail(el)) {
            el.focus();
            return;
        }
        document.getElementById('sForm').style.display = "none";
        document.getElementById('progress').style.display = "block";
        var request = {
            email: document.getElementById('emaiForSubscription').value,
            topic: "jb.webide.announce"
        };
        SubscriptionService.submitSubscriptionRequest(request, {
            callback: function (result) {
                setMessage(result ? "You request has been processed! Please check your mailbox." : "Internal error.");
            },
            timeout: 30000,
            errorHandler:function(message) {
                setMessage("Oops: " + message);
            }
        });
        function setMessage(text) {
            var span = document.getElementById("formContainer");
            span.style.display = "block";
            span.innerHTML = text;
            document.getElementById('progress').style.display = "none";
        }
    }
    function doBlurOn(el) {
        if (el.value == '') {
            el.value = 'Email PhpStorm news to me';
        }
    }
    function doFocusOn(el) {
        /*alert("ss");*/
        if (el.value == 'Email PhpStorm news to me') {
            el.value = '';
        }
    }
</script>
<dl id="submitionContent" class="subcription">
    
    <dd id="formContainer">
        <form action="javascript:get();" name="sForm" id="sForm">
            <input class="text required" name="emaiForSubscription" id="emaiForSubscription" value="Email PhpStorm news to me" onblur="doBlurOn(this)" onfocus="doFocusOn(this)"/>
            <input type="submit" class="submit" id="button" value="Subscribe" onclick="Proceed();"/>
            <span class="msg" id="error-msg" style="display:none;">Incorrect e-mail address</span>
        </form>
    </dd>
    <dd id="progress" style="display:none;"></dd>
</dl>

